<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Unit Test Snippet</Title>
      <Description>Code snippet for a unit test method</Description>
      <Shortcut>test</Shortcut>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>Subject</ID>
          <ToolTip>Replace with the object under test.</ToolTip>
          <Default>Subject</Default>
        </Literal>
        <Literal>
          <ID>Name</ID>
          <ToolTip>Replace with the name of the test.</ToolTip>
          <Default>TestName</Default>
        </Literal>
      </Declarations>
      <Code Language="CSharp">
        <![CDATA[[Subject(typeof($Subject$))]
	class $Name$
	{
    $end$
	}]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>